      <dst>$DART_COMMON_DIR/run_lookups.py</dst>
    </dir>
    <dir>
      <src>/home/tcondie/phi/phi/tests/simple_lookup</src>
      <dst>$DART_COMMON_DIR/simple_lookup</dst>
    </dir>
  </commonfiles>

  <preexecution>
    <script>$DART_COMMON_DIR/reset.sh</script>
  </preexecution>

  <execution duration="1800">

    <nodegroup>
      <nodes>0</nodes>
      <cmd>$DART_COMMON_DIR/run_node.py -i $DART_MY_IP -p 10000 $DART_COMMON_DIR/runChord $DART_MY_OUTPUT_DIR</cmd>
    </nodegroup>

    <nodegroup>
      <nodes>1</nodes>
      <cmd time="5">$DART_COMMON_DIR/run_node.py -i $DART_MY_IP -p 10000 -l $DART_MASTER:10000 -n 10 $DART_COMMON_DIR/runChord $DART_MY_OUTPUT_DIR</cmd>
    </nodegroup>

    <nodegroup>
      <nodes>2</nodes>
      <cmd time="10">$DART_COMMON_DIR/run_node.py -i $DART_MY_IP -p 10000 -l $DART_MASTER:10000 -n 10 $DART_COMMON_DIR/runChord $DART_MY_OUTPUT_DIR</cmd>
    </nodegroup>

    <nodegroup>
      <nodes>3-5</nodes>
      <cmd time="15">$DART_COMMON_DIR/run_node.py -i $DART_MY_IP -p 10000 -l $DART_MASTER:10000 -n 10 $DART_COMMON_DIR/runChord $DART_MY_OUTPUT_DIR</cmd>
    </nodegroup>

    <nodegroup>
      <nodes>6-9</nodes>
      <cmd time="20">$DART_COMMON_DIR/run_node.py -i $DART_MY_IP -p 10000 -l $DART_MASTER:10000 -n 10 $DART_COMMON_DIR/runChord $DART_MY_OUTPUT_DIR</cmd>
    </nodegroup>

    <nodegroup>
      <nodes>10-19</nodes>
      <cmd time="25">$DART_COMMON_DIR/run_node.py -i $DART_MY_IP -p 10000 -l $DART_MASTER:10000 -n 10 $DART_COMMON_DIR/runChord $DART_MY_OUTPUT_DIR</cmd>
    </nodegroup>

  </execution>

  <postexecution>
    <script>$DART_COMMON_DIR/reset.sh</script>
  </postexecution>

  <reset>
    <script>$DART_COMMON_DIR/reset.sh</script>
  </reset>

</dart>
